{
  "name": "react4xp",
  "version": "0.6.2-SNAPSHOT",
  "description": "React4xp starter for Enonic XP7.0",
  "scripts": {
    "rmnode": "node_modules/rimraf/bin.js node_modules || true",
    "install:dev": "nsi",
    "install:prod": "npm install",
    "clean": "./gradlew clean --refresh-dependencies  --info",
    "build": "./gradlew build --info",
    "deploy": "./gradlew deploy --info",
    "rebuild": "npm run clean && npm run build",
    "redeploy": "npm run rebuild && npm run deploy",
    "dev": "npm run redeploy && npm run watch:all",
    "watch": "npm-run-all --parallel watch:xp watch:r4xp",
    "watch:all": "npm-run-all --parallel watch:gradle watch:xp watch:r4xp",
    "watch:gradle": "./gradlew deploy --info -t",
    "watch:r4xp": "webpack --env.REACT4XP_CONFIG_FILE=build/react4xp_constants.json --config node_modules/react4xp-build-components/webpack.config.js --progress --watch",
    "watch:xp": "babel src/main/resources --out-dir build/resources/main --ignore **/*.jsx --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enonic/starter-react4xp.git"
  },
  "author": "Espen Norderud",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/enonic/starter-react4xp/issues"
  },
  "homepage": "https://github.com/enonic/starter-react4xp#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react4xp-build-components": "0.7.3",
    "react4xp-buildconstants": "0.8.0",
    "react4xp-regions": "0.2.1",
    "react4xp-runtime-client": "0.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "fibers": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "npm-safe-install": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "react4xp-runtime-externals": "0.4.0",
    "react4xp-runtime-nashornpolyfills": "0.4.3",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-arrow-functions"
    ]
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
